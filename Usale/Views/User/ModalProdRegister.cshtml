
@{
    ViewData["Title"] = "Registro";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link rel="stylesheet" href="~/css/style.css" />
<link rel="stylesheet" href="~/css/master.css" />

<!-- Vendor CSS Files -->
<link href="~/vendor/icofont/icofont.min.css" rel="stylesheet">
<link href="~/vendor/line-awesome/css/line-awesome.min.css" rel="stylesheet">
<link href="~/vendor/aos/aos.css" rel="stylesheet">
<link href="~/vendor/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">


<style>

    div .modal {
        margin: 180px 0 0 450px;
        display: block;
        width: 600px;
        height: 400px;
        background-color: white;
        border: 1px solid rgba(0,0,0,0.2);
    }

    .textContent {
        margin-top: 70px;
    }
</style>

<main id="main">
    <div class="container">
        <div class="modal">
            <div class="modal-header text-center">
                <h5 class="modal-title w-100">Registro de Perfil</h5>
            </div>
            <div class="text-center mt-5">
                <h5>Escolha o tipo de perfil que mais combina com você!</h5>
            </div>
            <div class="container text-center d-flex justify-content-center flex-column w-50 textContent">
                <a class="btn btn-primary" asp-action="CadastroClienteView" asp-controller="User">Manual</a>
                <a class="btn btn-info mt-2" asp-action="CadastroVendedorView" asp-controller="User">Excel</a>
            </div>
        </div>
    </div>
</main>

<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>

